{
  "name": "egg-timer-project",
  "version": "1.0.0",
  "description": "Simple Egg Timer desktop app.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:mac": "electron-builder --mac",
    "build:win": "electron-builder --win"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ben07br/egg-timer.git"
  },
  "keywords": [],
  "author": "ben07br",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ben07br/egg-timer/issues"
  },
  "homepage": "https://github.com/ben07br/egg-timer#readme",
  "devDependencies": {
    "electron": "^37.2.5"
  },
  "build": {
    "appId": "com.ben07br.eggtimer",
    "productName": "EggTimer",
    "files": [
      "**/*",
      "!dist",
      "!node_modules/.cache"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "portable"
    }
  }
}
